<%- include("partials/header"); -%>

<div class="contact">

  <form action="/" class="form-signin" method="POST">

    <h1 class="h3 mb-3 fw-normal">Contact Me!</h1>

    <div class="form-floating">
      <input type="text" name="fName" class="form-control top" placeholder="First Name" required autofocus>
      <label for="floatingInput"></label>
    </div>
    <div class="form-floating">
      <input type="text" name="lName" class="form-control middle" placeholder="Last Name">
      <label for="floatingInput"></label>
    </div>
    <div class="form-floating">
      <input type="email" name="email" class="form-control bottom" placeholder="Email">
      <label for="floatingPassword"></label>
    </div>


    <button class="w-100 btn btn-lg btn-primary" type="submit">Submit!</button>

  </form>

</div>




<%- include("partials/footer"); -%>
